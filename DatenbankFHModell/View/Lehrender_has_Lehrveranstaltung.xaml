<Window x:Class="DatenbankFHModell.Lehrender_has_Lehrveranstaltung"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatenbankFHModell"
        mc:Ignorable="d"
        Title="Lehrender - Lehrveranstaltung" Height="250" MinHeight="250" Width="250">
    <Grid Background="LightGray">
        <StackPanel Margin="5">
            <Label>Personalnummer:</Label>
            <TextBox x:Name="txt_Personalnummer"></TextBox>
            <Label>Lehrveranstaltungsnummer:</Label>
            <TextBox x:Name="txt_Lehrveranstaltungsnummer"></TextBox>
            <Label>Fakultätsnummer:</Label>
            <TextBox x:Name="txt_Fakultaetsnummer"></TextBox>
            <Button Height="25" Margin="0,10,0,0" x:Name="btn_Insert" Click="btn_Insert_Click">Hinzufügen</Button>
            <Button Height="25" Margin="0,10,0,0" x:Name="btn_Close" Click="btn_Close_Click">Schließen</Button>
        </StackPanel>
    </Grid>
</Window>
